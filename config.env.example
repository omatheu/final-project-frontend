# Configurações do Projeto Visagio
# =================================

# API Key do Google Gemini (OBRIGATÓRIO)
GOOGLE_API_KEY=your_gemini_api_key_here

# Configurações do Banco de Dados
DATABASE_PATH=Bases_VAI - oficial real.db

# Configurações da API RAG
API_TITLE=Visagio RAG API
API_VERSION=1.0.0
MODEL_NAME=gemini-2.5-flash
TEMPERATURE=0.0
SIMILARITY_THRESHOLD=0.7

# Configurações do Frontend
NEXT_PUBLIC_RAG_API_URL=http://localhost:8000

# Configurações do PostgreSQL (opcional)
POSTGRES_PASSWORD=visagio_password

# Configurações de Ambiente
NODE_ENV=development 